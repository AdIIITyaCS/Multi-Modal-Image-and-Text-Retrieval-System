# Configuration for Multi-Modal Retrieval System

# Flask Configuration
FLASK_APP=app.py
FLASK_ENV=development
FLASK_PORT=5000

# Model Configuration
TEXT_MODEL=sentence-transformers/clip-ViT-B-32-multilingual-v1
IMAGE_MODEL=clip-ViT-B-32
EMBEDDING_DIMENSION=512

# FAISS Configuration
FAISS_INDEX_PATH=./data/indices/
NPROBE=10

# Data Configuration
DATA_PATH=./data/corpus/
MAX_CORPUS_SIZE=10000

# System Configuration
BATCH_SIZE=32
MAX_IMAGE_SIZE=224
